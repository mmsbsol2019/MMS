                        </div>
                        <!-- <div class="row">
                         
                          <div class="col-md-4">
                            <mat-form-field appearance="outline">
                              <mat-label>MNC/NC/OBS</mat-label> 
                              <mat-select formControlName="mnc_nc_obs"  [(ngModel)]="editDeficiency.DEFICIENCY_TYPE">
