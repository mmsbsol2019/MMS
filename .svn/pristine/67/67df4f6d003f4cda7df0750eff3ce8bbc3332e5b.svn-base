    FIND_DEF:'SELECT FIN_TAB.* from ( SELECT @row_number:=CASE WHEN @DEFICIENCY_ID = ORG_TAB.DEFICIENCY_ID THEN @row_number + 1 ELSE 1 END AS ROWNUM,@DEFICIENCY_ID:=ORG_TAB.DEFICIENCY_ID as TEMP_DATA, ORG_TAB.*FROM (SELECT DEF_REF.*,MA_DEF.CODE_ID,MA_DEF.CODE_REF_ID ,MA_DEF.ROOT_CAUSE_ANALYSIS ,MA_DEF.CAUSE_ID, MA_DEF.DEFICIENCY_TYPE ,A_CODE.CODE_NAME,A_CODE_REF.CODE_REF_VAL, MA_CAUSE.CAUSE_VAL FROM MA_AUDIT_INFO INFO,MA_AUDIT_DEFICIENCY MA_DEF,AUDIT_DEFICIENCY_REF DEF_REF,ma_audit_code A_CODE, ma_audit_code_ref A_CODE_REF,ma_deficiency_cause MA_CAUSE WHERE MA_DEF.COMPANY_ID = ? AND DEF_REF.DEFICIENCY_ID = MA_DEF.DEFICIENCY_ID AND MA_DEF.AUDIT_TRACKING_NUMBER = INFO.AUDIT_TRACKING_NUMBER AND INFO.AUDIT_CATEGORY = ? AND INFO.AUDIT_TYPE = ? AND INFO.AUDIT_INTERVAL = ? order by DEF_REF.DEFICIENCY_ID) ORG_TAB ORDER BY  DEFICIENCY_ID desc) FIN_TAB where FIN_TAB.ROWNUM=1',
    FIND_DEF_HISTORY:'SELECT DEF.* FROM AUDIT_DEFICIENCY_REF DEF, MA_AUDIT_DEFICIENCY MA_DEF WHERE MA_DEF.COMPANY_ID = ? AND DEF.DEFICIENCY_ID LIKE ? GROUP BY TXN_SEQ_NO ORDER BY TXN_SEQ_NO DESC',
