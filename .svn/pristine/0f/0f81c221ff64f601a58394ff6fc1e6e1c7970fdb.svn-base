ipcMain.on('check-username-password-request', (event, data) => {
  conn.getConnection()
    .then((connection) => {
      crud.find(connection, Constants.FIND_LOGIN_USER, data)
        .then((value) => {
          event.sender.send('check-username-password-response', value)
          connection.release();
        })
        .catch(() => {
          console.log('error');
        })

    });
})

